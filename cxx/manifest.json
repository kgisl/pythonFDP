{
    "name" : "C++ Search",
    "description" : "Adds support to the omnibox to search the C++ reference material.",
    "manifest_version" : 2, 
	"background" : {
         "scripts" : [ "background.js" ]
    },    
    "icons" : { "128" : "icon128.png", "32" : "icon32.png", "16" : "icon16.png" },
    "omnibox" : { "keyword" : "cxx" },
    "permissions" : [
        "tabs",
        "*://*.sgi.com/",
        "*://*.ac.be/",
        "*://cplusplus.com/",
        "*://*.cppreference.com/"
    ],
    "version" : "2.0"
}
